#include "stdafx.h"
#include <iostream>
#include "conio.h"

using namespace std;

bool numeromultiplo(int n,int i);

int main () {
	int n,i;
	bool total;
	cout<<" ingrese numero";
	cin>> n;
	cout<<" ingrese otro numero";
	cin>> i;
	total= numeromultiplo(n,i);
    if (total==true)
		cout<<" es multiplo";
	else 
		cout<<" no es multiplo";

	getch ();
}
bool numeromultiplo(int n,int i){
	bool aux=false;
	if(n>i)
		if (n%i==0)
			return true  ;
		else 
			return false;
	else
		if(i%n!=0)
			return true;
		else
			return false;
	return aux;
}
